matriz = []
N = int(input("Qual a dimensão da matriz (NxN)? "))


for i in range(N):
    Linha = []
    for j in range(N):
        elemento = int(input(f"Digite o valor da linha {i} coluna {j}: "))
        Linha.append(elemento)
    matriz.append(Linha)


matriz_identidade = True
for i in range(N):
    for j in range(N):
        if i == j:   
            if matriz[i][j] != 1:
                matriz_identidade = False
        else:  
            if matriz[i][j] != 0:
                matriz_identidade = False


print("A matriz é identidade?" , matriz_identidade)


for i in range(N):
    for j in range(N):
        print(matriz[i][j], end=" ")
    print()